<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>PigObjFsLib</RootNamespace>
    <TargetFrameworks>net5.0;net45;net40;netcoreapp3.1;</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <Authors>Seow Phong</Authors>
    <Company>Seow Phong Studio</Company>
    <Description>Interface similar to Microsoft Scripting Runtime of VB6.</Description>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageProjectUrl>https://en.seowphong.com/oss/PigObjFs/</PackageProjectUrl>
    <PackageReleaseNotes>An interface that can be used under .net 5.0 is similar to the class library of Microsoft Scripting Runtime of VB6.</PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/PhongSeow/PigObjFs</RepositoryUrl>
    <PackageTags>Microsoft Scripting Runtime</PackageTags>
    <NeutralLanguage />
    <Copyright>Copyright (c) 2020 Seow Phong</Copyright>
    <Version>1.0.1</Version>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
